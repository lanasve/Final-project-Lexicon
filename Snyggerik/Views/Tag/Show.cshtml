@model IEnumerable<Snyggerik.Models.Tag>

@{
    ViewBag.Title = "Show";
}
<div class="container body-content">
    
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
           
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                
            </tr>
        }

    </table>
    </div>